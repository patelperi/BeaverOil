<?xml version="1.0" standalone="yes"?>
<xs:schema id="DSTransporterFacilities" targetNamespace="http://www.tempuri.org/DSTransporterFacilities.xsd" xmlns:mstns="http://www.tempuri.org/DSTransporterFacilities.xsd" xmlns="http://www.tempuri.org/DSTransporterFacilities.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DSTransporterFacilities" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="tblTransporters">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransporterID" msdata:AutoIncrement="true" type="xs:int" minOccurs="0" />
              <xs:element name="TransporterName" type="xs:string" />
              <xs:element name="TAddress1" type="xs:string" minOccurs="0" />
              <xs:element name="TAddress2" type="xs:string" minOccurs="0" />
              <xs:element name="TCity" type="xs:string" minOccurs="0" />
              <xs:element name="TState" type="xs:string" minOccurs="0" />
              <xs:element name="TZip" type="xs:string" minOccurs="0" />
              <xs:element name="TPhone" type="xs:string" minOccurs="0" />
              <xs:element name="TContactName" type="xs:string" minOccurs="0" />
              <xs:element name="TContactPhone" type="xs:string" minOccurs="0" />
              <xs:element name="T_USEPAID" type="xs:string" minOccurs="0" />
              <xs:element name="Active" type="xs:boolean" minOccurs="0" />
              <xs:element name="T_ILNumber" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:tblTransporters" />
      <xs:field xpath="mstns:TransporterName" />
    </xs:unique>
  </xs:element>
</xs:schema>